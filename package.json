{
  "name": "nexinterface",
  "version": "1.2.1",
  "description": "A collection of web components based on nexwidget and nexstate following material design guidelines.",
  "main": "dist/nexinterface.js",
  "type": "module",
  "module": "dist/nexinterface.js",
  "engines": {
    "node": ">=14.6.0",
    "npm": ">=6.0.0"
  },
  "files": [
    "dist/**/*",
    "assets/**/*"
  ],
  "scripts": {
    "build": "npx tsc && npx prettier dist/**/*.{js,d.ts} --write",
    "postversion": "git push && git push --tags && npm publish",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hawmex/nexinterface.git"
  },
  "keywords": [
    "ui",
    "nexwidget",
    "nexstate",
    "material"
  ],
  "author": "Hamed Aarab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hawmex/nexinterface/issues"
  },
  "homepage": "https://github.com/Hawmex/nexinterface#readme",
  "dependencies": {
    "nexstate": "^4.1.2",
    "nexwidget": "^4.3.3"
  },
  "devDependencies": {
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  }
}
